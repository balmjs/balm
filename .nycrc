{
  "extension": [
    ".ts"
  ],
  "include": [
    "src/**/*.ts",
    "!src/config/env.ts",
    "!src/tasks/index.ts",
    "!src/tasks/private/sprite.ts",
    "!src/tasks/private/modernizr.ts",
    "!src/tasks/public/server.ts",
    "!src/middlewares/index.ts",
    "!src/plugins/*.ts",
    "!src/utilities/address.ts",
    "!src/utilities/detect-port.ts",
    "!src/utilities/escape-string-regexp.ts",
    "!src/utilities/replacestream.ts",
    "!src/utilities/vinyl-sourcemaps-apply.ts"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**",
    "types/**/*.d.ts",
    "test/**/*.spec.ts"
  ],
  "reporter": [
    "lcov",
    "text",
    "text-summary"
  ]
}

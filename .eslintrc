{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "globals": {
    "path": true,
    "gulp": true,
    "$": true,
    "browserSync": true,
    "reload": true,
    "config": true,
    "Task": true,
    "noop": true,
    "RN": true,
    "mix": true,
    "collections": true
  },
  "extends": [
    "eslint:recommended",
    "xo-space"
  ],
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "generator-star-spacing": 0
  }
}

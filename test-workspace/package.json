{
  "name": "balm-test",
  "version": "2020.8.3",
  "author": "Elf-mousE <ifmouz@gmail.com>",
  "scripts": {
    "preinstall": "bower install",
    "dev": "balm",
    "prod": "balm -p",
    "php:dev": "balm --config config/php",
    "php:prod": "balm -p --config config/php",
    "mpa:dev": "balm --config ./config/mpa",
    "mpa:prod": "balm -p --config config/mpa",
    "spa:dev": "balm --config config/spa",
    "spa:prod": "balm -p --config config/spa",
    "pwa:dev": "balm --config config/pwa",
    "pwa:prod": "balm -p --config config/pwa",
    "vue:dev": "balm --config config/vue-spa",
    "vue:prod": "balm -p --config config/vue-spa",
    "ssr:dev": "balm --config config/vue-ssr/client.js",
    "ssr:build:client": "balm -p --config config/vue-ssr/client.js",
    "ssr:build:server": "balm -p -ssr --config config/vue-ssr/server.js",
    "ssr:prod": "npm run ssr:build:client && npm run ssr:build:server",
    "ssr:start": "cross-env NODE_ENV=production node vue-ssr/server.js",
    "ts:dev": "balm --config config/ts",
    "ts:prod": "balm -p --config config/ts",
    "esnext:dev": "balm --config config/esnext",
    "esnext:prod": "balm -p --config config/esnext",
    "mp:dev": "balm --config config/mp",
    "mp:build:web": "balm -p --config config/mp",
    "mp:build:mp": "balm -p -mp --config config/mp",
    "mp:prod": "npm run mp:build:web && npm run mp:build:mp"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "kbone-api": "^0.2.1",
    "lib-flexible": "^0.3.2",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.0",
    "@babel/plugin-proposal-numeric-separator": "^7.10.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/runtime-corejs3": "^7.10.0",
    "css-loader": "^4.2.0",
    "less": "^3.12.0",
    "less-loader": "^6.2.0",
    "mp-webpack-plugin": "^1.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-pxtorem": "^5.1.1",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-node-externals": "^2.5.0"
  },
  "peerDependencies": {
    "balm": ">=3"
  },
  "private": true
}

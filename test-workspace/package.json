{
  "name": "balm-test",
  "version": "2019.12.20",
  "author": "Elf-mousE <ifmouz@gmail.com>",
  "scripts": {
    "preinstall": "bower install",
    "dev": "gulp",
    "prod": "gulp -p",
    "php:dev": "gulp --cwd . -f config/php",
    "php:prod": "gulp -p --cwd . -f config/php",
    "mpa:dev": "gulp --cwd . -f config/mpa",
    "mpa:prod": "gulp -p --cwd . -f config/mpa",
    "spa:dev": "gulp --cwd . -f config/spa",
    "spa:prod": "gulp -p --cwd . -f config/spa",
    "pwa:dev": "gulp --cwd . -f config/pwa",
    "pwa:prod": "gulp -p --cwd . -f config/pwa",
    "vue:dev": "gulp --cwd . -f config/vue-spa",
    "vue:prod": "gulp -p --cwd . -f config/vue-spa",
    "ssr:dev": "gulp --cwd . -f config/vue-ssr/client.js",
    "ssr:build:client": "gulp -p --cwd . -f config/vue-ssr/client.js",
    "ssr:build:server": "gulp -p -ssr --cwd . -f config/vue-ssr/server.js",
    "ssr:prod": "npm run ssr:build:client && npm run ssr:build:server",
    "ssr:start": "cross-env NODE_ENV=production node vue-ssr/server.js",
    "ts:dev": "gulp --cwd . -f config/ts",
    "ts:prod": "gulp -p --cwd . -f config/ts",
    "esnext:dev": "gulp --cwd . -f config/esnext",
    "esnext:prod": "gulp -p --cwd . -f config/esnext"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.7.4",
    "axios": "^0.19.0",
    "core-js": "^3.4.7",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-node-externals": "^1.7.2"
  },
  "private": true
}

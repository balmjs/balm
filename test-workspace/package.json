{
  "name": "balm-test",
  "version": "2020.4.26",
  "author": "Elf-mousE <ifmouz@gmail.com>",
  "scripts": {
    "preinstall": "bower install",
    "dev": "gulp",
    "prod": "gulp -p",
    "php:dev": "gulp --cwd . -f config/php",
    "php:prod": "gulp -p --cwd . -f config/php",
    "mpa:dev": "gulp --cwd . -f config/mpa",
    "mpa:prod": "gulp -p --cwd . -f config/mpa",
    "spa:dev": "gulp --cwd . -f config/spa",
    "spa:prod": "gulp -p --cwd . -f config/spa",
    "pwa:dev": "gulp --cwd . -f config/pwa",
    "pwa:prod": "gulp -p --cwd . -f config/pwa",
    "vue:dev": "gulp --cwd . -f config/vue-spa",
    "vue:prod": "gulp -p --cwd . -f config/vue-spa",
    "ssr:dev": "gulp --cwd . -f config/vue-ssr/client.js",
    "ssr:build:client": "gulp -p --cwd . -f config/vue-ssr/client.js",
    "ssr:build:server": "gulp -p -ssr --cwd . -f config/vue-ssr/server.js",
    "ssr:prod": "npm run ssr:build:client && npm run ssr:build:server",
    "ssr:start": "cross-env NODE_ENV=production node vue-ssr/server.js",
    "ts:dev": "gulp --cwd . -f config/ts",
    "ts:prod": "gulp -p --cwd . -f config/ts",
    "esnext:dev": "gulp --cwd . -f config/esnext",
    "esnext:prod": "gulp -p --cwd . -f config/esnext",
    "mp:dev": "gulp --cwd . -f config/mp",
    "mp:build:web": "gulp -p --cwd . -f config/mp",
    "mp:build:mp": "gulp -p -mp --cwd . -f config/mp",
    "mp:prod": "npm run mp:build:web && npm run mp:build:mp"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.2",
    "axios": "^0.19.2",
    "balm-ui": "^5.6.3",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "jquery": "^3.5.0",
    "kbone-api": "^0.2.1",
    "lib-flexible": "^0.3.2",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.3.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "mp-webpack-plugin": "^0.9.13",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-pxtorem": "^5.1.1",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-node-externals": "^1.7.2"
  },
  "private": true
}

{
  "name": "balm-test",
  "version": "2019.8.23",
  "author": "Elf-mousE <ifmouz@gmail.com>",
  "scripts": {
    "preinstall": "bower install",
    "dev": "gulp",
    "prod": "gulp -p",
    "mpa:dev": "gulp -f config/mpa/index.js",
    "mpa:prod": "gulp -p -f config/mpa/index.js",
    "pwa:dev": "gulp -f config/pwa/index.js",
    "pwa:prod": "gulp -p -f config/pwa/index.js",
    "vue:dev": "gulp -f config/vue-spa/index.js",
    "vue:prod": "gulp -p -f config/vue-spa/index.js",
    "ssr:dev": "gulp -f config/vue-ssr/client.js",
    "build:client": "gulp -p -f config/vue-ssr/client.js",
    "build:server": "gulp -p -ssr -f config/vue-ssr/server.js",
    "ssr:prod": "npm run build:client && npm run build:server",
    "ssr:start": "cross-env NODE_ENV=production node vue-ssr/server.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-typescript": "^7.3.3",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "typescript": "^3.5.3",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-node-externals": "^1.7.2"
  },
  "private": true
}
